    <property name="Northing" type="freetext">
      <description/>
    </property>
    <property name="Easting" type="freetext">
      <description/>
    </property>
    <property name="Accuracy" type="freetext">
      <description/>
    </property>
    <property name="Heading" type="freetext">
      <description/>
    </property>
    <property name="Elevation_Absolute" type="freetext">
      <description/>
    </property>
    <property name="Elevation_Range" type="freetext">
      <description/>
    </property>
    <property name="Landscape" type="enum">
      <description/>
      <lookup>
        <term> {Hiltop}
          <description/>
        </term>
        <term> {Ridge}
          <description/>
        </term>
        <term> {Base_of_Ridge}
          <description/>
        </term>
        <term> {Base_of_Hill}
          <description/>
        </term>
        <term> {Cliff}
          <description/>
        </term>
        <term> {Other}
          <description/>
        </term>
