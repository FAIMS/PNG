    <property name="Vegetation" type="enum">
      <description/>
      <lookup>
        <term> {Primary_Forest}
          <description/>
        </term>
        <term> {Secondary_Forest}
          <description/>
        </term>
        <term> {Grassland}
          <description/>
        </term>
        <term> {Garden}
          <description/>
        </term>
        <term> {Scrub_Undergrowth}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Ground_Visibility" type="enum">
      <description/>
