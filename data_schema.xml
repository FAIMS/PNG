    <property name="Created_At" type="freetext">
      <description/>
    </property>
    <property name="Author" type="freetext">
      <description/>
    </property>
    <property name="Team" type="freetext">
      <description/>
    </property>
    <property name="Local_Site_Name" type="freetext">
      <description/>
    </property>
    <property name="Site_Type" type="enum">
      <description/>
      <lookup>
        <term> {Ethnographic}
          <description/>
        </term>
        <term> {Surface_Scatter}
          <description/>
        </term>
        <term> {Rockshelter}
          <description/>
        </term>
        <term> {Isolated_Feature_Artefact}
          <description/>
        </term>
        <term> {Other}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Site_Significance" type="enum">
      <description/>
      <lookup>
        <term> {None}
          <description/>
        </term>
        <term> {Low}
          <description/>
        </term>
        <term> {Medium}
          <description/>
        </term>
        <term> {High}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="Land_Owners" type="freetext">
      <description/>
