<?xml version="1.0" encoding="UTF-8"?>
<dataSchema>
  <ArchaeologicalElement name="Project" type="enum">
    <description></description>
    <property name="Team_Members" type="freetext">
      <description/>
    </property>
    <property name="Author" type="freetext">
      <description/>
    </property>
    <property name="Site_Code" type="freetext">
      <description/>
    </property>
    <property name="Site_Number" type="measure">
      <description/>
    </property>
    <property name="Records" type="enum">
      <description/>
      <lookup>
        <term> {Records}
          <description/>
        </term>
      </lookup>
    </property>
    <property name="All_Site_Records" type="enum">
      <description/>
      <lookup>
        <term> {All_Site_Records}
          <description/>
        </term>
      </lookup>
    </property>
