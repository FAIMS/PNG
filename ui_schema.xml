<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>PNG</h:title>
    <model>
      <instance>
        <faims id="PNG">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <user>
            <usertab>
              <Area_Code/>
              <users/>
              <login/>
            </usertab>
          </user>
          <Users>
            <Login>
              <Author_List/>
            </Login>
          </Users>
          <Project>
            <Main>
              <Team_Members/>
              <Author/>
              <Col_Group_1>
                <Col_Group_10>
                  <Site_Code/>
                </Col_Group_10>
                <Col_Group_11>
                  <Site_Number/>
                </Col_Group_11>
              </Col_Group_1>
              <Record_Site/>
              <GPS_Diagnostics/>
            </Main>
            <Records>
              <All_Site_Records/>
            </Records>
            <Map>
              <map/>
              <container1>
                <child1>
                  <Centre_Map/>
                </child1>
                <child2>
                  <Save_Map_Settings/>
                </child2>
                <child3>
                  <Create_Shape/>
                </child3>
              </container1>
            </Map>
          </Project>
          <Site>
            <Site_Recording>
              <Col_Group_2>
                <Col_Group_20>
                  <Site_Code/>
                </Col_Group_20>
                <Col_Group_21>
                  <Site_Number/>
                </Col_Group_21>
              </Col_Group_2>
              <Created_At/>
              <Author/>
              <Team/>
              <Local_Site_Name/>
              <Site_Type/>
              <Site_Significance/>
              <Land_Owners/>
              <Informant_Details/>
              <Col_Group_3>
                <Col_Group_31>
                  <Latitude/>
                </Col_Group_31>
                <Col_Group_32>
                  <Longitude/>
                </Col_Group_32>
              </Col_Group_3>
              <Col_Group_4>
                <Northing/>
                <Easting/>
              </Col_Group_4>
              <Take_From_GPS/>
              <Col_Group_5>
                <Accuracy/>
                <Heading/>
              </Col_Group_5>
              <Elevation_Absolute/>
              <Elevation_Range/>
              <Landscape/>
              <Vegetation/>
              <Ground_Visibility/>
              <Current_Land_Use/>
              <Sampling_Method/>
              <Estimated_Dimensions/>
              <Cultural_Material_Present/>
              <Surface_Geomorphology/>
              <Subsurface_Geomorphology/>
              <Disturbance/>
              <Site_Description/>
              <General_Notes/>
              <Recommendation/>
              <Photo/>
              <Sketches/>
              <Attach_A_Sketch/>
            </Site_Recording>
            <Measurements>
              <Measurement_Type/>
              <Measurement/>
              <Add_A_Measurement/>
              <List_Of_Measurements/>
            </Measurements>
            <Photolog>
              <Record_External_Photo/>
              <List_Of_External_Photos/>
            </Photolog>
          </Site>
          <Photolog>
            <Photolog>
              <Photo_ID/>
              <Photo_Timestamp/>
              <Photo_Note/>
            </Photolog>
          </Photolog>
        </faims>
      </instance>
      <!-- Insert nodeset bindings here -->
      <!-- TODO: make this binding work: -->
      <bind type="decimal" nodeset="/faims/Project/Main/Col_Group_1/Col_Group_11/Site_Number"/>
    </model>
  </h:head>
  <h:body>
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
    </group>
    <group ref="Users">
      <label>{Users}</label>
      <group ref="Login">
        <label>{Login}</label>
        <select1 ref="Author_List" appearance="compact">
          <label>{Author_List}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Project">
      <label>{Project}</label>
      <group ref="Main">
        <label>{Main}</label>
        <input ref="Team_Members">
          <label>{Team_Members}</label>
        </input>
        <input ref="Author">
          <label>{Author}</label>
        </input>
        <group ref="Col_Group_1" faims_style="orientation">
          <label></label>
          <group ref="Col_Group_10" faims_style="even">
            <label></label>
            <input ref="Site_Code">
              <label>{Site_Code}</label>
            </input>
          </group>
          <group ref="Col_Group_11" faims_style="even">
            <label></label>
            <input ref="Site_Number">
              <label>{Site_Number}</label>
            </input>
          </group>
        </group>
        <trigger ref="Record_Site" faims_style_class="faims-blue">
          <label>{Record_Site}</label>
        </trigger>
        <input ref="GPS_Diagnostics" faims_web="false" faims_certainty="false" faims_annotation="false" faims_read_only="true" faims_style_class="gps-status">
          <label>{GPS_Diagnostics}</label>
        </input>
      </group>
      <group ref="Records">
        <label>{Records}</label>
        <select1 ref="All_Site_Records" appearance="compact">
          <label>{All_Site_Records}</label>
          <item>
            <label>No Records Yet</label>
            <value>placeholder</value>
          </item>
        </select1>
      </group>
      <group ref="Map" faims_scrollable="false">
        <label>{Map}</label>
         <input ref="map" faims_map="true" faims_certainty="false">
          <label/>
        </input>
        <group ref="container1" faims_style="orientation">
          <label/>
          <group faims_style="even" ref="child1">
            <label/>
            <trigger ref="Centre_Map" faims_style_class="faims-blue">
              <label>{Centre_Map}</label>
            </trigger>
          </group>
          <group faims_style="even" ref="child2">
            <label/>
            <trigger ref="Save_Map_Settings" faims_style_class="faims-blue">
              <label>{Save_Map_Settings}</label>
            </trigger>
          </group>
          <group faims_style="even" ref="child3">
            <label/>
            <trigger ref="Create_Shape" faims_style_class="faims-blue">
              <label>{Create_Shape}</label>
            </trigger>
          </group>
        </group>
      </group>
    </group>
    <group ref="Site">
      <label>{Site}</label>
      <group ref="Site_Recording">
        <label>{Site_Recording}</label>
        <group ref="Col_Group_2" faims_style="orientation">
          <label></label>
          <group ref="Col_Group_20" faims_style="even">
            <label></label>
            <input ref="Site_Code" faims_attribute_name="Site_Code" faims_attribute_type="freetext" faims_read_only="true" faims_certainty="false">
              <label>{Site_Code}</label>
            </input>
          </group>
          <group ref="Col_Group_21" faims_style="even">
            <label></label>
            <input ref="Site_Number" faims_attribute_name="Site_Number" faims_attribute_type="freetext" faims_read_only="true" faims_certainty="false">
              <label>{Site_Number}</label>
            </input>
          </group>
        </group>
        <input ref="Created_At" faims_read_only="true">
          <label>{Created_At}</label>
        </input>
        <input ref="Author" faims_read_only="true">
          <label>{Author}</label>
        </input>
        <input ref="Team" faims_attribute_name="Team" faims_attribute_type="freetext" faims_read_only="true">
          <label>{Team}</label>
        </input>
        <input ref="Local_Site_Name" faims_attribute_name="Local_Site_Name" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true">
          <label>{Local_Site_Name}</label>
        </input>
        <select1 ref="Site_Type" faims_attribute_name="Site_Type" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Site_Type}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="Site_Significance" appearance="full" faims_attribute_name="Site_Significance" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Site_Significance}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Land_Owners" faims_attribute_name="Land_Owners" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true">
          <label>{Land_Owners}</label>
        </input>
        <input ref="Informant_Details" faims_attribute_name="Informant_Details" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true">
          <label>{Informant_Details}</label>
        </input>
        <group ref="Col_Group_3" faims_style="orientation">
          <label></label>
          <group ref="Col_Group_31" faims_style="even">
            <label></label>
            <input ref="Latitude" faims_attribute_name="Latitude" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true" faims_read_only="true">
              <label>{Latitude}</label>
            </input>
          </group>
          <group ref="Col_Group_32" faims_style="even">
            <label></label>
            <input ref="Longitude" faims_attribute_name="Longitude" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true" faims_read_only="true">
              <label>{Longitude}</label>
            </input>
          </group>
        </group>
        <group ref="Col_Group_4">
          <label></label>
          <input ref="Northing" faims_attribute_name="Northing" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true" faims_read_only="true">
            <label>{Northing}</label>
          </input>
          <input ref="Easting" faims_attribute_name="Easting" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true" faims_read_only="true">
            <label>{Easting}</label>
          </input>
        </group>
        <trigger ref="Take_From_GPS" faims_style_class="faims-blue">
          <label>{Take_From_GPS}</label>
        </trigger>
        <group ref="Col_Group_5">
          <label></label>
          <input ref="Accuracy" faims_attribute_name="Accuracy" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true" faims_read_only="true">
            <label>{Accuracy}</label>
          </input>
          <input ref="Heading" faims_attribute_name="Heading" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true" faims_read_only="true">
            <label>{Heading}</label>
          </input>
        </group>
        <input ref="Elevation_Absolute" faims_attribute_name="Elevation_Absolute" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true">
          <label>{Elevation_Absolute}</label>
        </input>
        <input ref="Elevation_Range" faims_attribute_name="Elevation_Range" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true">
          <label>{Elevation_Range}</label>
        </input>
        <select1 ref="Landscape" faims_attribute_name="Landscape" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Landscape}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="Vegetation" faims_attribute_name="Vegetation" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Vegetation}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="Ground_Visibility" faims_attribute_name="Ground_Visibility" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Ground_Visibility}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="Current_Land_Use" faims_attribute_name="Current_Land_Use" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Current_Land_Use}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <select1 ref="Sampling_Method" faims_attribute_name="Sampling_Method" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Sampling_Method}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Estimated_Dimensions" faims_attribute_name="Estimated_Dimensions" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true">
          <label>{Estimated_Dimensions}</label>
        </input>
        <select ref="Cultural_Material_Present" faims_attribute_name="Cultural_Material_Present" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Cultural_Material_Present}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select>
        <select1 ref="Surface_Geomorphology" faims_attribute_name="Surface_Geomorphology" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Surface_Geomorphology}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Subsurface_Geomorphology" faims_attribute_name="Subsurface_Geomorphology" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true">
          <label>{Subsurface_Geomorphology}</label>
        </input>
        <select1 ref="Disturbance" faims_attribute_name="Disturbance" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Disturbance}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Site_Description" faims_attribute_name="Site_Description" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true">
          <label>{Site_Description}</label>
        </input>
        <input ref="General_Notes" faims_attribute_name="General_Notes" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true">
          <label>{General_Notes}</label>
        </input>
        <input ref="Recommendation" faims_attribute_name="Recommendation" faims_attribute_type="freetext" faims_certainty="true" faims_annotation="true">
          <label>{Recommendation}</label>
        </input>
        <group ref="Photo">
          <label>{Photo}</label>
        </group>
        <select ref="Sketches" faims_attribute_name="Sketches" faims_attribute_type="vocab" faims_certainty="true" faims_annotation="true">
          <label>{Sketches}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select>
        <trigger ref="Attach_A_Sketch" faims_style_class="faims-blue">
          <label>{Attach_A_Sketch}</label>
        </trigger>
      </group>
      <group ref="Measurements">
        <label>{Measurements}</label>
        <select1 ref="Measurement_Type" faims_attribute_name="Measurement_Type" faims_attribute_type="vocab">
          <label>{Measurement_Type}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
        <input ref="Measurement" faims_attribute_name="Measurement" faims_attribute_type="freetext">
          <label>{Measurement}</label>
        </input>
        <trigger ref="Add_A_Measurement" faims_style_class="faims-blue">
          <label>{Add_A_Measurement}</label>
        </trigger>
        <select1 ref="List_Of_Measurements" appearance="compact">
          <label>{List_Of_Measurements}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
      </group>
      <group ref="Photolog">
        <label>{Photolog}</label>
        <trigger ref="Record_External_Photo" faims_attribute_name="Record_External_Photo" faims_style_class="faims-blue">
          <label>{Record_External_Photo}</label>
        </trigger>
        <select1 ref="List_Of_External_Photos" appearance="compact">
          <label>{List_Of_External_Photos}</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Photolog">
      <label>{Photolog}</label>
      <group ref="Photolog">
        <label>{Photolog}</label>
        <input ref="Photo_ID" faims_attribute_name="Photo_ID" faims_attribute_type="freetext">
          <label>{Photo_ID}</label>
        </input>
        <input ref="Photo_Timestamp" faims_attribute_name="Photo_Timestamp" faims_attribute_type="freetext">
          <label>{Photo_Timestamp}</label>
        </input>
        <input ref="Photo_Note" faims_attribute_name="Photo_Note" faims_attribute_type="freetext" faims_certainty="true">
          <label>{Photo_Note}</label>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
